{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "hoai app",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${workspaceFolder}/array/hoai.c",
                "${workspaceFolder}/array/hoai_lib.c",
                "-lm",
                "-o",
                "${workspaceFolder}/array/hoai_app",
                "-Wall",
                "-Wextra",
                "-std=c99"
            ],
            "options": {
                "cwd": "${workspaceFolder}/array/"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": "build",
            "detail": "Task generated by Debugger."
        },
        {
            "type": "cppbuild",
            "label": "C/C++: gcc build active file",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${file}",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}",
                "-Wall",
                "-Wextra",
                "-std=c99"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}